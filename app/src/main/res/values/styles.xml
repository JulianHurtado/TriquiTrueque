<?xml version="1.0" encoding="utf-8"?>

<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="SplashTeme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/splash_back</item>
        <item name="android:windowFullscreen">true</item>
    </style>



    <item name="actibar" type="color">#3A6EC3</item>
    <item name="darkblue" type="color">#00008b</item>

    <integer-array name="EditColors">
        <item>@color/actibar</item>
        <item>@color/darkblue</item>
    </integer-array>

</resources>
